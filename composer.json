{
  "name": "siteiran/otp-package",
  "description": "A Laravel package for OTP functionality.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Siteiran\\OtpPackage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siteiran\\OtpPackage\\OtpServiceProvider"
      ]
    }
